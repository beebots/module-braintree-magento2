{
    "name": "gene/module-braintree",
    "description": "Fork from the Magento Braintree 2.2.0 module by Gene Commerce for PayPal.",
    "version": "4.0.9",
    "type": "magento2-module",
    "license": "proprietary",
    "require": {
        "magento/module-customer": "103.0.*",
        "magento/module-config": "101.2.*",
        "magento/module-sales": "102.0.*||103.0.*",
        "magento/module-directory": "100.4.*",
        "magento/module-payment": "100.4.*",
        "magento/module-checkout": "100.4.*",
        "magento/module-backend": "102.0.*",
        "magento/module-vault": "101.2.*",
        "magento/module-catalog": "104.0.*",
        "magento/module-quote": "101.2.*",
        "magento/module-paypal": "101.0.*",
        "magento/module-theme": "101.1.*",
        "magento/module-ui": "101.2.*",
        "magento/module-multishipping": "100.4.*",
        "ext-json": "*",
        "ext-simplexml": "*",
        "php": ">=7.2.0 || ^8.1",
        "league/iso3166": "^2.1",
        "msp/recaptcha": "^2.2.3"
    },
    "replace": {
        "magento/module-braintree": "100.*.*",
        "braintree/braintree_php": "3.*"
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-0": {
            "Braintree": "braintree_php/lib/"
        },
        "psr-4": {
            "Magento\\Braintree\\": "",
            "Braintree\\": "braintree_php/lib/Braintree"
        }
    },
    "authors": [
        {
            "name": "Magento Commerce",
            "homepage": "http://www.magento.com",
            "role": "Original Developers"
        },
        {
            "name": "Aidan Threadgold",
            "email": "aidan@gene.co.uk",
            "homepage": "https://www.gene.co.uk",
            "role": "Developer"
        },
        {
            "name": "Paul Canning",
            "email": "paul.canning@gene.co.uk",
            "homepage": "https://www.gene.co.uk",
            "role": "Developer"
        }
    ]
}
